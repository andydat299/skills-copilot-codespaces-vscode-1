# Discord Bot Configuration - Thay thế bằng thông tin thật của bạn
DISCORD_TOKEN=your_bot_token_here
CLIENT_ID=your_client_id_here
GUILD_ID=your_guild_id_here

# MongoDB Configuration - Thay thế bằng MongoDB URI của bạn
MONGO_URI=your_mongodb_connection_string

# Admin Configuration - Thay thế bằng Discord User ID của bạn
ADMIN_IDS=your_discord_user_id_here

# Withdraw System Configuration (Railway overrides these)
ADMIN_CHANNEL_ID=your_admin_channel_id_here
ADMIN_ROLE_ID=your_admin_role_id_here

# Optional: Log Channel
LOG_CHANNEL_ID=your_log_channel_id

# Location System Settings
LOCATION_SYSTEM_ENABLED=true
